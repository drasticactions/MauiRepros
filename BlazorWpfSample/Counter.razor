<h1>Counter</h1>

<p>Current count: @currentCount</p>

<button class="btn btn-primary" @onclick="IncrementCount">Click me</button>

<button class="btn btn-primary" onclick="window.open('https://www.google.com', '_blank')">Website Link Test</button>

<button class="btn btn-primary" onclick="window.open('', '_blank')">Blank Window Test</button>

<a href="https://www.google.com" target="_blank">Google Link</a>

@code {
    private int currentCount = 0;

    private void IncrementCount()
    {
        currentCount++;
    }
}
